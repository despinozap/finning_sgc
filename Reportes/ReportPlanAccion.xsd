                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
		pa.detalle_correccion,
		pa.fecha_correccion,
		CONVERT(VARCHAR(4), (CAST(pa.progreso AS VARCHAR) + CAST('%' AS VARCHAR))) AS progreso,
