          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="EvaluacionTableAdapter" GeneratorDataComponentClassName="EvaluacionTableAdapter" Name="Evaluacion" UserDataComponentName="EvaluacionTableAdapter">
